function accounts() {
  export RR=$HOME/projects/streamsend-accounts
  alias rr='cd $RR'
  alias serve='script/rails server'
  alias con='script/rails console'
  cd $RR
}

function ss() {
  export RR=$HOME/projects/streamsend
  alias rr='cd $RR'
  alias serve='script/server'
  alias con='script/console'
  cd $RR
}

function share() {
  export RR=$HOME/projects/streamsend-share
  alias rr='cd $RR'
  alias serve='foreman start'
  alias con='bin/console'
  cd $RR
}

function shareable() {
  export RR=$HOME/projects/shareable
  alias rr='cd $RR'
  alias serve='script/rails server'
  alias con='script/rails console'
  cd $RR
}

function billing-api() {
  export RR=$HOME/projects/billing-api
  alias rr='cd $RR'
  alias serve='script/server'
  alias con='script/console'
  cd $RR
}

function billing() {
  export RR=$HOME/projects/billing
  alias rr='cd $RR'
  alias serve='script/server'
  alias con='script/console'
  cd $RR
}
